           li
             a(href='tables.html') Tables
           li
             a(href='forms.html') Forms
           li
             a(href='buttons.html') Buttons
      
      
           li
             a(href='tooltips.html') Tooltips
           li
             a(href='messages.html') Messages
           li
             a(href='badge.html') Badges
           li
             a(href='thumbnail.html') Thumbnail
      
           li
             a(href='navbar.html') Navbar
           li
             a(href='list.html') List  Group
           li
             a(href='nav.html') Nav
           li
             a(href='subnav.html') Subnav
           li
