/*  Desktop and up */

@media (min-width: screenDesktop) 

    .row 
        max-width: ( screenDesktop  - (gutterWidth * 2) )
        center-block()
    

    [class*="desktop-"]    
        float: left
        padding-right: (gutterWidth / 2)
    
    [class*="desktop-"]:last-child
        padding-right 0

    //call here
    generate-grid("desktop")

    generate-offset("desktop") 

    generate-pull("desktop")
    generate-push("desktop")

