doctype html
html
  head
    block title
      title Default title
      
      
    include header
    
    

  body
  
    include _nav
    
    mixin top_left(vals)
      ul.link-list.active-list
        each val in items
          li
            a(href='##{val.replace(/ /g,"")}') #{val}
    
    .row

       .mobile-12.tablet-3.desktop-2.large-2
        
         block leftcontent
        
         h3 Css Components 
         ul.link-list
           li
