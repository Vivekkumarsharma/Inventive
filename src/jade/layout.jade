doctype html
html
  head
    block title
      title Default title
     
 
    include header

   
  body
  
    include _nav
    
    mixin top_left(vals)
      ul.link-list.active-list
        each val in items
          li
            a(href='##{val.replace(/ /g,"")}') #{val}
    
    .row

       .mobile-12.tablet-3.desktop-2.large-2.hidden-mobile
        
         block leftcontent
        
         h3 Css Components
        
         - var links = ['Typography','Grid' ,'Tables', 'Forms' , 'Buttons', 'Tooltips' ,'Messages', 'Badges', 'Thumbnail','Navbar','Lists','Nav','Subnav','Panel','Breadcrumb','Pagination','Progress','Search'] 
         ul.link-list.left-navigations
          each val in links
            - var l = val.replace(/ /g,"-").toLowerCase()   
            li(class="m-#{l}")
              a(href='#{l}.html') #{val}
            
          
       .mobile-12.tablet-9.desktop-9.large-9.col-push-1
         block content
